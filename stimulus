      var Verb = function Verb(_ref) {
        var _this = this;

        var _ref$meaning = _ref.meaning,
            meaning = _ref$meaning === void 0 ? "" : _ref$meaning,
            _ref$meanings = _ref.meanings,
            meanings = _ref$meanings === void 0 ? [] : _ref$meanings,
            _ref$rePoliteForm = _ref.rePoliteForm,
            rePoliteForm = _ref$rePoliteForm === void 0 ? "" : _ref$rePoliteForm,
            _ref$politeForm = _ref.politeForm,
            politeForm = _ref$politeForm === void 0 ? "" : _ref$politeForm,
            _ref$kanji = _ref.kanji,
            kanji = _ref$kanji === void 0 ? {
          word: "",
          meaning: ""
        } : _ref$kanji,
            hasLearned = _ref.hasLearned,
            shouldKnow = _ref.shouldKnow,
            timesWrong = _ref.timesWrong,
            timesRight = _ref.timesRight;

        _classCallCheck(this, Verb);

        this.increaseTimesCorrect = function (callback) {
          _this.timesRight++;
          if (!_this.hasLearned) _this.toggleLearned();

          if (_this.timesRight > 0 + _this.timesWrong + _this.timesWrongOffset && !_this.shouldKnow) {
            _this.toggleKnown();
          }

          if (callback) return callback(_this);
        };

        this.increaseTimesWrong = function (callback) {
          _this.timesWrong++;
          if (_this.shouldKnow) _this.toggleKnown();
          if (callback) return callback(_this);
        };

        this.toggleKnown = function () {
          return _this.shouldKnow = !_this.shouldKnow;
        };

        this.toggleLearned = function () {
          return _this.hasLearned = !_this.hasLearned;
        };

        this.timesWrongOffset = 3; //     change to array of meanings, allow for comma separated entries for multiple meanings and multiple points

        this.meanings = meanings || null;
        this.meaning = meaning || null;
        this.politeForm = politeForm || null;
        this.rePoliteForm = rePoliteForm || null;
        this.hasLearned = hasLearned || false;
        this.shouldKnow = shouldKnow || false;
        this.timesWrong = timesWrong || 0;
        this.timesRight = timesRight || 0;
        this.kanji = kanji;
      };

